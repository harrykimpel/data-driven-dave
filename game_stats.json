          "visualization": {
            "id": "viz.markdown"
          },
          "rawConfiguration": {
            "text": "# Data Driven Dave ![New Relic logo](https://newrelic.com/static-assets/images/icons/avatar-newrelic.png)\n## WeAreDevelopers World Congress Challenge\n![Data Driven Dave screenshot](https://raw.githubusercontent.com/devreldragon/data-driven-dave/main/Screenshots/game_screenshot-level01.png)"
          }
        },
        {
          "title": "Games Played",
          "layout": {
            "column": 7,
            "row": 1,
            "width": 2,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.billboard"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountIds": [
                  YOUR_ACCOUNT_ID
                ],
                "query": "SELECT sum(game_complete) AS 'Games' FROM Span WHERE service.name = 'data-driven-dave' SINCE 7 days ago"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
        {
          "title": "Top Score Achieved",
          "layout": {
            "column": 9,
            "row": 1,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.billboard"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountIds": [
                  YOUR_ACCOUNT_ID
                ],
                "query": "FROM Span SELECT max(player_score) where service.name = 'data-driven-dave' since 7 DAYS AGO"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
        {
          "title": "Maximum Level Reached",
          "layout": {
            "column": 1,
            "row": 4,
            "width": 6,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.bullet"
          },
          "rawConfiguration": {
            "limit": 10,
            "nrqlQueries": [
              {
                "accountIds": [
                  YOUR_ACCOUNT_ID
                ],
                "query": "FROM Span SELECT max(current_level) AS 'Level' where service.name = 'data-driven-dave'  since 7 DAYS AGO"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
        {
          "title": "Top Score by Level",
          "layout": {
            "column": 7,
            "row": 4,
            "width": 6,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.table"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountIds": [
                  YOUR_ACCOUNT_ID
                ],
                "query": "  FROM Span SELECT max(player_score) AS 'Top Score' where service.name = 'data-driven-dave' facet current_level as 'Level'  since 7 DAYS AGO  "
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
        {
          "title": "Items Collected",
          "layout": {
            "column": 1,
            "row": 7,
            "width": 12,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.area"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountIds": [
                  YOUR_ACCOUNT_ID
                ],
                "query": "FROM Span SELECT count(*) where collected_item_type is NOT NULL facet collected_item_type timeseries since 15 minutes ago"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        }
      ]
